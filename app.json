        "ITSAppUsesNonExemptEncryption": false
      },
      "bundleIdentifier": "com.anonymous.AffWorldLoginApp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.AffWorldLoginApp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "1101dd75-7282-41b6-929d-7c66831f8e75"
      }
    }
  }
}
